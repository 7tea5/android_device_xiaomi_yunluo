# dcxo
genfscon sysfs /kernel/clk_buf/ u:object_r:sysfs_dcxo:s0

# Extcon usb nodes
genfscon sysfs /devices/platform/extcon_usb1 u:object_r:sysfs_extcon:s0
genfscon sysfs /devices/platform/extcon_usb u:object_r:sysfs_extcon:s0
genfscon sysfs /devices/platform/soc/soc:extcon_usb/extcon u:object_r:sysfs_extcon:s0

# Graphics
genfscon sysfs /devices/platform/soc/13000000.mali u:object_r:sysfs_gpu:s0

